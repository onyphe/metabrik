"Welcome to Plashy - version $Plashy::VERSION"

set global ctimeout 20
set global rtimeout 20

load www

load slurp

load httpproxy

load ssh2
set ssh2 debug 1
set ssh2 host localhost
set ssh2 username gomor
set ssh2 publickey /home/gomor/.ssh/id_rsa.pub
set ssh2 privatekey /home/gomor/.ssh/id_rsa

load vfeed
set vfeed db /home/pkpf420/plashydev/data/vfeed.db
